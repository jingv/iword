<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iword"
    android:versionCode="1"
    android:versionName="1.0" >
    
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity 
            android:name=".login"
            android:label="iWord" >
           <intent-filter>
               <action android:name="android.intent.action.MAIN" />
               <category android:name="android.intent.category.LAUNCHER" />
           </intent-filter>
        </activity>
       <activity android:name=".registered" > </activity>
       <activity android:name=".iword" > </activity>
       <activity android:name=".word_remember_start" > </activity>
       <activity android:name=".aboutiword" > </activity>
       <activity android:name=".changekey" > </activity>
    </application>

</manifest>
